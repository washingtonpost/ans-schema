{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://raw.githubusercontent.com/washingtonpost/ans-schema/master/src/main/resources/schema/ans/0.5.6/story_elements/oembed.json",
  "description": "An oembed object",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "type": {
      "enum": [ "oembed" ]
    },
    "_id": {
      "$ref": "https://raw.githubusercontent.com/washingtonpost/ans-schema/master/src/main/resources/schema/ans/0.5.6/traits/trait_id.json"
    },
    "subtype": {
      "$ref": "https://raw.githubusercontent.com/washingtonpost/ans-schema/master/src/main/resources/schema/ans/0.5.6/traits/trait_subtype.json"
    },
    "channels": {
      "$ref": "https://raw.githubusercontent.com/washingtonpost/ans-schema/master/src/main/resources/schema/ans/0.5.6/traits/trait_channel.json"
    },
    "additional_properties": {
      "$ref": "https://raw.githubusercontent.com/washingtonpost/ans-schema/master/src/main/resources/schema/ans/0.5.6/traits/trait_additional_properties.json"
    },

    "providerUrl": {
      "description": "The endpoint for the provider's oembed API.",
      "type": "string"
    },
    "objectUrl": {
      "description": "The canonical url of the embeddable object.",
      "type": "string"
    }
  },
  "required": ["type", "providerUrl", "objectUrl"]
}
